{  
   "kind":"List",
   "apiVersion":"v1",
   "metadata":{  
      "name":"reen.example.com example"
   },
   "items":[  
      {  
         "kind":"Route",
         "apiVersion":"v1",
         "metadata":{  
            "name":"route-reen"
         },
         "spec":{  
            "host":"reen.example.com",
            "to":{  
               "kind":"Service",
               "name":"service-secure"
            },
            "port":{  
               "targetPort":"http"
            }
         }
      },
      {  
         "kind":"Service",
         "apiVersion":"v1",
         "metadata":{  
            "name":"service-secure"
         },
         "spec":{  
            "ports":[  
               {  
                  "name":"https",
                  "protocol":"TCP",
                  "port":8080,
                  "targetPort":8080
               }
            ],
            "type": "NodePort",
            "selector":{  
               "name":"caddy-docker"
            }
         }
      },
      {  
         "kind":"Pod",
         "apiVersion":"v1",
         "metadata":{  
            "name":"caddy-docker-1",
            "labels":{  
               "name":"caddy-docker"
            }
         },
         "spec":{  
            "containers":[  
               {  
                  "name":"caddy-docker-pod",
                  "image":"bmeng/caddy-docker",
                  "ports":[  
                     {  
                        "containerPort":8080,
                        "protocol":"TCP"
                     },
                     {  
                        "containerPort":8443,
                        "protocol":"TCP"
                     }
                  ]
               }
            ]
         }
      }
   ]
}
