{  
   "kind":"List",
   "apiVersion":"v1",
   "metadata":{  
      "name":"hello-service-complete-example"
   },
   "items":[  
      {  
         "kind":"Route",
         "apiVersion":"v1",
         "metadata":{  
            "name":"hello-route",
            "labels":{  
               "name":"hello-route-1"
            }
         },
         "spec":{  
            "host":"hello-openshift.com",
            "to":{  
               "kind":"Service",
               "name":"hello-service"
            }
         }
      },
      {  
         "kind":"Service",
         "apiVersion":"v1",
         "metadata":{  
            "name":"hello-service"
         },
         "spec":{  
            "selector":{  
               "name":"hello-openshift"
            },
            "ports":[  
               {  
                  "protocol":"TCP",
                  "port":8080,
                  "targetPort":8080
               }
            ]
         }
      },
      {  
         "apiVersion":"v1",
         "kind":"ReplicationController",
         "metadata":{  
            "labels":{  
               "name":"hello-openshift"
            },
            "name":"hello-test"
         },
         "spec":{  
            "replicas":2,
            "template":{  
               "metadata":{  
                  "labels":{  
                     "name":"hello-pods"
                  }
               },
               "spec":{  
                  "containers":[  
                     {  
                        "image":"openshift/hello-openshift",
                        "name":"hello-pods",
                        "ports":[  
                           {  
                              "containerPort":8080
                           },
                           {  
                              "containerPort":443
                           }
                        ]
                     }
                  ]
               }
            }
         }
      }
   ]
}
