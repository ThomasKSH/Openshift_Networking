
{
  "kind": "Pod",
  "apiVersion":"v1beta3",

  "metadata": {
  	"name": "hello-pod-1",
  	"labels": {
    		"name": "hello-openshift"
  	}
  },

  "spec": {
      "containers": [{
        "name": "hello-pod-container-1",
        "image": "openshift/hello-openshift",
        "ports": [
          {
            "containerPort": 8080
          },
          {
            "containerPort": 443
          }
        ]
      }]
  }
}

