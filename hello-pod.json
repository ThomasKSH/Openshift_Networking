
{
  "kind": "Pod",
  "apiVersion":"v1beta3",

  "metadata": {
  	"name": "hello-openshift-pod",
  	"labels": {
    		"name": "hello-openshift"
  	}
  },

  "spec": {
      "containers": [{
        "name": "hello-openshift-pod-container",
        "image": "pweil/hello-nginx-docker",
        "ports": [
          {
            "containerPort": 80
          },
          {
            "containerPort": 443
          }
        ]
      }]
  }
}
